{
  "extends": ["tslint-microsoft-contrib"],
  "rules": {
    "export-name": false,
    "newline-per-chained-call": false,
    "no-relative-imports": false,
    "no-shadowed-variable": false,
    "no-submodule-imports": [true, "rxjs"],
    "prefer-type-cast": false,
    "trailing-comma": true,
    "typedef": false
  }
}
