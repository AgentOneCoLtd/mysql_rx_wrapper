{
  "name": "@ag1/mysql_rx_wrapper",
  "version": "1.0.0",
  "description": "use mysql lib in rx style",
  "main": "./lib/index.js",
  "jsnext:main": "./lib-module/index.js",
  "module": "./lib-module/index.js",
  "typings": "./lib/index.d.ts",
  "author": "agent one co., ltd.",
  "license": "MIT",
  "private": false,
  "scripts": {
    "tsc:lib": "tsc -b tsconfig.json",
    "tsc:lib-module": "tsc -b tsconfig-module.json",
    "compile": "yarn tsc:lib && yarn tsc:lib-module",
    "lint:onlyts": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "lint:spects": "tslint -c tslint-spects.json -p tsconfig-spects.json -t stylish -e \"**/!(*.spec).ts\"",
    "lint": "yarn lint:onlyts --fix && yarn lint:spects --fix",
    "prettier:config": "prettier --no-config --write \"./*.{json,yml}\"",
    "prettier:ts": "prettier --write \"./src/**/*.ts\"",
    "prettier": "yarn prettier:config && yarn prettier:ts",
    "test": "jest --config \"./jest.config.json\"",
    "build": "yarn prettier && yarn lint && yarn compile && yarn test"
  },
  "peerDependencies": {
    "mysql": "^2.16.0",
    "rxjs": "^6.4.0"
  },
  "dependencies": {
    "@ag1/empty_object": "^1.0.0",
    "@ag1/return_switch": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/mysql": "^2.15.5",
    "jest": "^24.5.0",
    "mysql": "^2.16.0",
    "prettier": "^1.16.4",
    "rxjs": "^6.4.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.3333"
  },
  "keywords": [
    "mysql",
    "rxjs",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/AgentOneCoLtd/mysql_rx_wrapper.git",
    "type": "git"
  },
  "files": [
    "lib",
    "lib-module",
    "src"
  ]
}
