{
  "name": "@ag1/mysql_rx_wrapper",
  "version": "0.6.0",
  "description": "use mysql lib in rx style",
  "main": "index.js",
  "author": "agent one co., ltd.",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn lint && yarn compile",
    "compile": "tsc",
    "lint": "tslint -p tsconfig.json -t stylish",
    "test": "jest --env=node",
    "prettier": "yarn prettier-json && yarn prettier-ts",
    "prettier-json": "prettier --no-config --write './*.json'",
    "prettier-ts": "prettier --write './**/*.ts'"
  },
  "peerDependencies": {
    "mysql": "^2.16.0",
    "rxjs": "^6.2.0"
  },
  "dependencies": {
    "@ag1/empty_object": "^0.1.0",
    "@ag1/return_switch": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/mysql": "^2.15.5",
    "jest": "^23.4.2",
    "mysql": "^2.16.0",
    "prettier": "^1.14.2",
    "rxjs": "^6.2.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.0",
    "typescript": "^3.0.1"
  },
  "keywords": [
    "mysql",
    "rxjs",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/AgentOneCoLtd/mysql_rx_wrapper.git",
    "type": "git"
  },
  "readmeFilename": "readme.md",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.spec.+(ts)"
    ]
  }
}
